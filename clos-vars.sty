%% Theorems and definitions
\theoremstyle{plain}
\newtheorem{theorem}{Theorem}
\newtheorem{proposition}[theorem]{Proposition}
\newtheorem{corollary}[theorem]{Corollary}
\newtheorem{claim}[theorem]{Claim}
\newtheorem{lemma}[theorem]{Lemma}
\newtheorem{axiom}[theorem]{Axiom}

\newtheorem*{corollary*}{Corollary}
\newtheorem*{claim*}{Claim}
\newtheorem*{lemma*}{Lemma}
\newtheorem*{proposition*}{Proposition}
\newtheorem*{theorem*}{Theorem}

\theoremstyle{definition}
\newtheorem{definition}{Definition}
\newtheorem{example}{Examples}
\newtheorem{examples}[example]{Example}

\newtheorem*{example*}{Example}

\newtheoremstyle{exercise}
    {24pt plus 4pt minus 4pt} % Space above
    {12pt plus 4pt minus 4pt} % Space below
    {} % Body font
    {} % Indent amount
    {\bfseries} % Theorem head font
    {.} % Punctuation after theorem head
    {.5em} % Space after theorem head
    {} % Theorem head spec (can be left empty, meaning ‘normal’)

\theoremstyle{exercise}
\newtheorem{exercise}{Exercise}
\newtheorem{problem}[exercise]{Problem}

\newtheorem*{exercise*}{Exercise}
\newtheorem*{problem*}{Problem}

\theoremstyle{remark}
\newtheorem{remark}{Remark}
\newtheorem{remarks}[remark]{Remarks}

\newtheorem*{remark*}{Remark}
\newtheorem*{remarks*}{Remarks}

\makeatletter
\@ifundefined{solution}{%
\newenvironment{solution}[1][Solution]{%
  \proof[\textnormal{\bfseries{#1}}]%
  \ensuremath{\scriptstyle\blacktriangleright}
}{
\renewcommand\qedsymbol{\ensuremath{\scriptstyle\blacktriangleleft}}
\endproof}
}{}
\makeatother

\newenvironment{subproof}[1][Proof]{
\begin{quote}
\proof[\textnormal{\textit{#1}}]%
}{
